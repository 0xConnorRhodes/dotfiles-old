#!/usr/bin/env bash
# note, everything executed in this script is locked to workspace 9

xset b off &
xinput --set-prop "SynPS/2 Synaptics TouchPad" "libinput Accel Speed" .2
xinput --set-prop "TPPS/2 IBM TrackPoint" "libinput Accel Speed" .2
dunst &
emacs --daemon &
urxvtd &
#espanso daemon &
pcmanfm -d &
mpd &
unclutter --timeout 1 --jitter 5 --ignore-scrolling &
syncthing -no-browser -logfile=/home/connor/.cache/syncthing/syncthing.log &
kdeconnect-indicator &
xset r rate 300 45 &
picom &
exec /usr/lib/polkit-gnome/polkit-gnome-authentication-agent-1 &
exec ~/.local/config_nosync/bin/autorun_nosync.sh &
exec /home/connor/bin/init-keyboard.sh &
playerctld &
calcurse --daemon &
redshift &
insync start &
sleep 6
~/bin/notifications-on.sh
~/bin/refbar
